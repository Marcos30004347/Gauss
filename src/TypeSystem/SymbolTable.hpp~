#ifndef SYMBOL_TABLE
#define SYMBOL_TABLE

#include <cstddef>

struct symbol_registry {
	char** data;
	long** tree;

	size_t* size;
	size_t* capacity;
};

symbol_registry *symbol_registry_create();

void symbol_registry_destroy(symbol_registry *table);

void symbol_registry_set_entry(symbol_registry *table, const char *id, size_t val);

size_t symbol_registry_get_entry(symbol_registry *table, const char *id);

#endif
